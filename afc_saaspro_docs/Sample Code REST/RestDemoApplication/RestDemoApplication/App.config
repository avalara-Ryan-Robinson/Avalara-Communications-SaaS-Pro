<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <appSettings>
    <add key="DefaultWorkingDir" value="e:\logs\RestDemo" />
    <add key="DefaultUsername" value="Username" />
    <add key="DefaultPassword" value="Password" />
    <add key="ClientId" value="536" />
    <add key="ClientProfileId" value="1" />
    <add key="DefaultUrl" value="https://communicationsdev.avalara.net" />
